---
import {ANALYTICS} from 'weekly:config';

const isProduction = import.meta.env.MODE === 'production';
---

{
    isProduction && ANALYTICS?.umami?.id ? (
        <script
            is:inline
            async
            defer
            data-website-id={ANALYTICS.umami.id}
            src='https://umami.mengpeng.tech/script.js'
        />
    ) : null
}
